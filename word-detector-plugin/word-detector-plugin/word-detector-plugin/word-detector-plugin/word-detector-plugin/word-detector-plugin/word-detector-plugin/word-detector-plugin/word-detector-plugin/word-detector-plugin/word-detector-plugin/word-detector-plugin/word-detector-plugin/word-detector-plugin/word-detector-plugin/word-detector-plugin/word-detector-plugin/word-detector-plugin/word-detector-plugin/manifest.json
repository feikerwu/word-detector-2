{
    "manifest_version": 2,

    "name": "word-detector",

    "description": "谷歌浏览器取词",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./detector.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}